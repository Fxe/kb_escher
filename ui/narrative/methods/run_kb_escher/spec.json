        "advanced": false,
        "allow_multiple": true,
        "with_border": true
      }
    ],
    "parameters": [ 
{
      "id": "report_height",
      "optional": false,
      "advanced": true,
      "allow_multiple": false,
      "default_values": [
        800
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [],
        "validate_as":"int",
        "min_int": 500,
        "max_int": 6000
      }
    },
           {
      "id": "user_map_id",
      "optional": true,
      "advanced": false,
      "allow_multiple": true,
      "default_values": ["null"],
      "field_type": "text",
      "text_options": {"valid_ws_types": ["KBaseFBA.EscherMap"]}
    },
    {
      "id": "model_id",
      "optional": false,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseFBA.FBAModel"
        ]
      }
    },
      {
      "id": "model_alias",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": []
      }
    },
        {
      "id": "object_ids",
      "optional": true,
      "advanced": false,
      "allow_multiple": false,
      "default_values": [
        ""
      ],
      "field_type": "text",
      "text_options": {
        "valid_ws_types": [
          "KBaseFBA.FBA", "KBaseFeatureValues.ExpressionMatrix", "KBaseMatrices.ChemicalAbundanceMatrix"
        ]
      }
    }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "kb_escher",
            "method": "run_kb_escher",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "report_height",
                    "target_property": "report_height"
                },{
                    "input_parameter": "user_map_id",
                    "target_property": "user_map_id"
                },{
                    "input_parameter": "map_data",
                    "target_property": "map_data"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
